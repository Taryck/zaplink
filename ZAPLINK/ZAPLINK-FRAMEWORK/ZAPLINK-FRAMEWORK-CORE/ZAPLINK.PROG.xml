REPORT  zaplink.

INCLUDE zaplink_top.
INCLUDE zaplink_scr.

*/----------------------main------------------------------------------\
START-OF-SELECTION.
************* N U G G E T *************
*   create empty nugget
  IF nugc = &apos;X&apos;.
    PERFORM nugget_create.
*   add object to nugget
  ELSEIF nuga = &apos;X&apos;.
    PERFORM nugget_add.
*   import nugget
  ELSEIF nugi = &apos;X&apos;.
    PERFORM nugget_import.
*   display objects in a nugget
  ELSEIF nugd = &apos;X&apos;.
    PERFORM nugget_display.
*   Un install a nugget
  ELSEIF nugu = &apos;X&apos;.
    PERFORM nugget_uninstall.
  ELSEIF nugp = &apos;X&apos;.
    PERFORM nugget_package.
  ELSEIF nugr = &apos;X&apos;.
    PERFORM nugget_tr.
  ENDIF.

END-OF-SELECTION.
