   <ITEM CMPNAME="NODE_2_RAW" VERSION="1" EXPOSURE="2" STATE="1" EDITORDER="6 " AUTHOR="TBENSIAL" CREATEDON="20091125" CHANGEDBY="TBENSIAL" CHANGEDON="20091125" MTDDECLTYP="1" MTDNEWEXC="X" R3RELEASE="700">
