     <ITEM SCONAME="ZAPLINK_CX_CONNECTOR" VERSION="1" LANGU="E" DESCRIPT="ZAPlink ROOT Exception for Connectors" EDITORDER="1 " AUTHOR="TBENSIAL" CREATEDON="20091125">
