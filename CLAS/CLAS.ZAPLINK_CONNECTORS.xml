   <ITEM CMPNAME="RESET_CONNECTORS" VERSION="1" EXPOSURE="2" STATE="1" EDITORDER="10 " AUTHOR="TBENSIAL" CREATEDON="20100114" CHANGEDBY="TBENSIAL" CHANGEDON="20100114" MTDDECLTYP="1" R3RELEASE="700">
